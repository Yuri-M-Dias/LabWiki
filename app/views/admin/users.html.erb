<p>Admin Users</p>
<table>
  <tr>
    <th width="20%">E-mail</th>
    <th width="20%">Username</th>
    <th width="20%">Signed Up On</th>
    <th width="20%">Last Signed In On</th>
  </tr>
  <% for user in @users %>
    <tr>
      <td><%= user.email %></td>
      <td><%= user.username %></td>
      <td><%= user.signed_up_on %></td>
      <td><%= user.last_signed_in_on %></td>
      <td><%= link_to 'Delete', user, :confirm => 'Are you sure?Really?!', :method => :delete %></td>
    </tr>
  <% end %>
</table>