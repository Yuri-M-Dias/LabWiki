  context.fillRect (0,0,170,60);                                      // draw a red box on the whole of the canvas
  context.fillStyle = 'green';                                       // set the color to 'yellow'
  context.fillRect (5,5,160,50);                                      // draw a yellow box inside the red one
  context.fillStyle = 'red';                                          // set the color back to 'red'
  context.font = '30pt Arial Black';                                        // define the CSS font for writing text
  context.fillText ('Seibah',10,50);                                   // write the text 'Hello'
</script>

